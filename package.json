{
  "name": "file-chunks-uploader",
  "version": "1.0.0",
  "keywords": ["file", "upload", "chunk", "promise", "multipart", "form"],
  "author": "Xiaoooyooo <xiaoooyooo@gmail.com>",
  "description": "Upload (large) file by chunks",
  "main": "dist/ejs/index.js",
  "scripts": {
    "build": "npm run build:cjs && npm run build:ejs",
    "build:cjs": "tsc -p config/tsconfig.cjs.json",
    "build:ejs": "tsc -p config/tsconfig.ejs.json",
    "dev": "tsc -w -p config/tsconfig.ejs.json"
  },
  "files": ["dist"],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/ejs/index.d.ts",
        "default": "./dist/ejs/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "license": "MIT",
  "devDependencies": {
    "typescript": "^4.8.4"
  }
}
